#----------------------------------------------------------------
# File cacheMemMake creates executable file cacheMemMake.
#----------------------------------------------------------------
#	Name: Karl Anthony James
#	Class: Operating Systems
#	Due: 12/5/2013
#	CacheMemory
#	A program to simulate a cache memory server.
#----------------------------------------------------------------
# Bind cacheMem.o
#----------------------------------------------------------------
cacheMem:	cacheMem.o
		g++ -o cacheMem cacheMem.o -lm
#----------------------------------------------------------------
# Compile main.cpp
#----------------------------------------------------------------
cacheMem.o:	main.cpp
		g++ -g -c main.cpp -o cacheMem.o